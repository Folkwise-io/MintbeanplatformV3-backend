{
  "name": "mintbean-platform-v3-backend",
  "version": "1.0.0",
  "private": "true",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "tsc",
    "predev": "yarn gen-types",
    "dev": "nodemon -x ts-node ./src/index.ts",
    "knex": "node -r dotenv/config ./node_modules/knex/bin/cli.js --knexfile ./src/db/knexfile.ts --knexpath ./src/db/",
    "gen-types": "graphql-codegen --config codegen.yml",
    "postgres": "docker-compose -f ./db/docker-compose.yml up",
    "psql": "docker exec -it db-mintbean-v3 psql -U mintbean -d mintbean_development_v3",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "apollo-server-express": "2.16.1",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "graphql": "15.3.0",
    "knex": "0.21.5",
    "pg": "8.3.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.17.8",
    "@graphql-codegen/typescript": "1.17.8",
    "@graphql-codegen/typescript-resolvers": "1.17.9",
    "@types/express": "4.17.7",
    "@types/node": "14.6.0",
    "nodemon": "2.0.4",
    "ts-node": "9.0.0",
    "typescript": "4.0.2"
  }
}
